{
  "name": "countdown-timer",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "cross-env PORT=5000 nodemon server.js",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "body-parser": "latest",
    "cross-env": "^7.0.3",
    "express": "latest",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "concurrently": "latest"
  }
}
